<mvc:View controllerName="sap.ui.walkthrough.index"
		  xmlns="sap.m"
		  xmlns:mvc="sap.ui.core.mvc">
	<Page title="{i18n>homePageTitle}">
		<headerContent>
			<Button icon="sap-icon://hello-world"
					press="onOpenDialog"/>
		</headerContent>
		<content>
			<mvc:XMLView viewName="sap.ui.walkthrough.app.overview.view.HelloPanel"/>
			<mvc:XMLView viewName="sap.ui.walkthrough.app.overview.view.InvoiceList"/>
		</content>
	</Page>
</mvc:View>